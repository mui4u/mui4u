{
  "name": "mui4u",
  "private": true,
  "workspaces": [
    "./src/*"
  ],
  "version": "0.0.1",
  "description": "MUI4U Components Monorepo",
  "main": "index.js",
  "repository": "https://github.com/mui4u/mui4u.git",
  "author": "Jordy Morales <dev.morales.jordy@gmail.com>",
  "license": "MIT",
  "scripts": {
    "syncpack": "syncpack list-mismatches",
    "typecheck": "tsc --noEmit",
    "storybook": "esno scripts/storybook-start",
    "lint": "eslint src --ext .ts,.tsx --cache",
    "build": "esno scripts/build",
    "build:all": "npm run build all",
    "jest": "jest",
    "prettier:test": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "clean": "rimraf src/*/{esm,cjs,lib,dist} src/**/*.tsbuildinfo",
    "test": "npm run prettier:test && npm run syncpack && npm run typecheck && npm run lint && npm run jest",
    "_release:no-test": "esno scripts/release",
    "_release": "npm run release:prepare && esno scripts/release",
    "_release:alpha": "npm run _release minor -- --stage alpha --tag next",
    "_release:alpha:major": "npm run _release:no-test major -- --stage alpha --tag next",
    "release:prepare": "npm run clean && npm test",
    "release:patch": "npm run _release patch",
    "release:minor": "npm run _release minor",
    "release:major": "npm run _release major"
  },
  "resolutions": {
    "@types/react": "18.0.9"
  },
  "dependencies": {
    "@emotion/react": "11.9.3",
    "@emotion/server": "11.4.0",
    "@emotion/styled": "^11.10.0",
    "react": "18.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-typescript": "^8.2.1",
    "@storybook/addons": "^6.5.10",
    "@storybook/api": "^6.5.10",
    "@storybook/components": "^6.5.10",
    "@storybook/core-events": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@storybook/theming": "^6.5.10",
    "@sucrase/jest-plugin": "^2.1.1",
    "@testing-library/dom": "8.13.0",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.2.0",
    "@testing-library/user-event": "14.2.0",
    "@types/fs-extra": "^9.0.11",
    "@types/jest": "^26.0.23",
    "@types/jest-axe": "^3.5.1",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.4",
    "@types/rimraf": "^3.0.1",
    "@types/yargs": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "chalk": "^4.1.1",
    "esbuild": "^0.12.5",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.18.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "esno": "^0.6.0",
    "execa": "^5.0.1",
    "fast-glob": "^3.2.5",
    "fs-extra": "^10.0.0",
    "is-directory": "^0.3.1",
    "jest": "^27.4.5",
    "jest-axe": "^4.1.0",
    "new-github-release-url": "^1.0.0",
    "open": "^8.2.0",
    "prettier": "^2.4.1",
    "react-dom": "18.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.50.5",
    "rollup-plugin-esbuild": "^4.2.3",
    "rollup-plugin-node-externals": "^2.2.0",
    "rollup-plugin-visualizer": "^5.5.0",
    "simple-git": "^2.39.0",
    "storybook-addon-outline": "^1.4.2",
    "storybook-addon-turbo-build": "^1.1.0",
    "storybook-dark-mode": "^1.1.0",
    "stylis": "^4.0.10",
    "syncpack": "^5.7.11",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "4.8.2",
    "yargs": "^17.0.1"
  }
}
